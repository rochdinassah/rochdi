#!/usr/bin/env node

// author: rochdi nassah
// created: 2025/09/23

'use strict';

const rochdi = require('../');

const { Discord } = rochdi;

const discord = new Discord(process.argv[2]);

discord.connect().then(ok => {
  if (!ok)
    exit('error');

  const { user } = discord;
  
  log('success(%s) | %s', user.bot ? 'bot' : 'user', user.global_name ?? user.username);
  discord.disconnect();
});