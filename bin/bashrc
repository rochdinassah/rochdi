#!/bin/bash

# author: rochdi nassah
# created: 2025/09/19

alias desktop='cd ~/Desktop'
alias c='clear'
alias cleartmp='sudo rm -rf /tmp/* /tmp/.*'
alias rochdi='desktop; cd rochdi; code .'
alias syn='echo ack'

if [ $(pwd) == $(echo ~) ]
then
  desktop
fi

extract() {
  # tar --file $1 --extract &&
  echo $1 | sed 's/\.[a-z].*//i'
}

mkdir() {
  /usr/bin/mkdir $1 && cd $1
}