#!/bin/bash

# author: rochdi nassah
# created: 2025/09/19

declare -A aliases

aliases[desktop]='cd ~/Desktop'
aliases[c]='clear'

while read line
do
  for alias in ${!aliases[@]}
  do
    if [[ $line =~ "alias $alias=" ]]
    then
      unset aliases[$alias]
      break 2
    fi
  done
done < ~/.bashrc

for alias in ${!aliases[@]}
do
  echo $alias
done