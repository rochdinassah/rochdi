#!/usr/bin/env node

'use strict';

const rochdi = require('../');
const crypto = require('node:crypto');
const tls = require('node:tls');
const WebSocket = require('ws');
const EE = require('node:events');
const ee = new EE();

const { Server, HttpClient, Http2Client } = rochdi;

const httpClient = new HttpClient();

const port = 65535;
const server = new Server({ port });

server.run();

server.get('/a/{key}/b/{key2}/c/{key3}', (req, res) => {
  return log('/a/{key}/b/{key2}/c/{key3}');
});

server.get('/tokens', (req, res) => {
  return log('/tokens');
});

server.get('/tokens/{id}', (req, res) => {
  return log('/tokens/{id}');
});

require('child_process').exec('curl 127.1:'+port+'/a/16/b/32/c/64');